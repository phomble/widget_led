{
  "id": "com.sap.custom.ledgeomap",
  "version": "1.0.0",
  "name": "LedGeoMap",
  "description": "LedGeoMap widget",
  "newInstancePrefix": "LedgeoMap",
  "icon": "data:image/png;base64,",
  "vendor": "sap",
  "eula": "",
  "license": "",
  "imports": [ "input-controls" ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-custom-ledgeomap",
      "url": "https://phomble.github.io/widget_led/simple.js",
      "integrity": "",
      "ignoreIntegrity": true
    },
   
  ],
  "properties": {
    "metadata": {
      "type": "string",
      "description": "For internal use",
      "default": ""
    },
    "portalurl": {
      "type": "string",
      "description": "ESRI Portal URL",
      "default": ""
    },
    "apikey": {
      "type": "string",
      "description": "Key",
      "default": ""  
   },
    "hstoken": {
      "type": "string",
      "description": "HS Token",
      "default": "00"  
    },
    "servicelevel": {
      "type": "integer",
      "description": "Electrical NODISCONT Service Level",
      "default": 0
    }
  },
  "methods": {
    "setServiceLevel": {
      "description": "Set the customer Service Level",
      "parameters": [
        {
          "name": "service",
          "type": "integer",
          "description": "ServiceLevel"
        }
      ],
      "body": "this.servicelevel = service;"
    },
    "getAPIkey": {   
      "returnType": "string",
      "description": "Optional api method - Get API Key",
      "parameters": [
      ],
      "body": "return this.apikey;"
    },
    "setAPIkey": {
      "description": "Set the API Key",
      "parameters": [
        {
          "name": "pkey",
          "type": "string",
          "description": "Set New API Key"
        }
      ],
      "body": "this.apikey = pkey;"
    },
      "setHSToken": {
      "description": "Set the HS Token",
      "parameters": [
        {
          "name": "hst",
          "type": "string",
          "description": "Set New HS TOken"
        }
      ],
      "body": "this.hstoken = hst;"
    },
      "getHSToken": {   
      "returnType": "string",
      "description": "hs token",
      "parameters": [
      ],
      "body": "return this.hstoken;"
    },
    "getPortalURL": {
      "returnType": "string",
      "description": "Get portal URL",
      "parameters": [
      ],
      "body": "return this.portalurl;"
    },
    "setPortalURL": {
      "description": "Set the Portal URL",
      "parameters": [
        {
          "name": "url",
          "type": "string",
          "description": "New Portal URL"
        }
      ],
      "body": "this.portalurl = url;"
    }
  },
  "events": {
    "onStart": {
      "description": "onStart events trigger"
    }
  }
}
